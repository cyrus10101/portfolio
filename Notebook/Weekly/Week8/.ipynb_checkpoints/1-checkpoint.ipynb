   "source": [
    "def nl(file):\n",
    "    try:\n",
    "        with open(file, \"r\") as file:\n",
    "            for i, line in enumerate(file, start=1):\n",
    "                print(f\"{i}\\t{line.rstrip()}\")\n",
    "    except FileNotFoundError:\n",
    "        print(\"File not found.\")"
   ]
  },
