    "#2\n",
    "def check_password(pw, cpw):\n",
    "    return pw == cpw\n",
    "\n",
    "password = input(\"Passwowrd(case sensetive): \")\n",
    "c_password = input(\"Confirm Passwowrd(case sensetive): \")\n",
    "\n",
    "if check_password(password, c_password):\n",
    "    print(\"Password Set\")\n",
    "else:\n",
    "    print(\"Error! Password not matched\")\n"
