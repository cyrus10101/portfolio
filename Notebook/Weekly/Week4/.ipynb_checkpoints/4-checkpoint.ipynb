    "#4\n",
    "def remove_last_char(s):\n",
    "    if len(s) <= 1:\n",
    "        return s\n",
    "    return s[:-1]\n",
    "\n",
    "string = input(\"Enter a string: \")\n",
    "result = remove_last_char(string)\n",
    "\n",
    "print(f\"String after removing last character: '{result}'\")\n",
    "\n",
    "\n"
