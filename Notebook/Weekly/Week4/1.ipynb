    "def validate_integer(num):\n",
    "    if num >= 0 and num <= 100:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "\n",
    "if validate_integer(int(input(\"Enter a integer:\"))):\n",
    "    print(\"The integer is in the range 0 to 100.\")\n",
    "else:\n",
    "    print(\"The integer is out of range.\")"
